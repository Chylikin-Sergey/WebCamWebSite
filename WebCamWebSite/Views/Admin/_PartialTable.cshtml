@model IEnumerable<WebCamWebSite.Models.Info>
@{
    foreach (var item in Model)
    {
        <tr>
            <td><input type="checkbox" /></td>
            <td>
                @item.Id
            </td>
            <td>
                @item.Name
                @item.Surname
            </td>
            <td>
                @item.Phone
            </td>
            <td>
                @item.Message
            </td>
            <td>
                @item.Date
            </td>
        </tr>
    }
}
